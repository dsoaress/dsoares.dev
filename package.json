{
  "name": "dsoares-site",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "backend": "yarn workspace backend dev",
    "frontend": "yarn workspace frontend dev",
    "start": "concurrently --kill-others-on-fail \"yarn backend\"  \"yarn frontend\"",
    "build": "yarn workspace frontend build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml}\""
  },
  "workspaces": [
    "frontend"
  ],
  "dependencies": {
    "concurrently": "^5.3.0"
  },
  "devDependencies": {
    "prettier": "^2.1.2"
  },
  "engines": {
    "node": "12.x"
  }
}
