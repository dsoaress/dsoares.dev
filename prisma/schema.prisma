generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Track {
  id        Int      @id @default(autoincrement())
  title     String
  artist    String
  album     String
  duration  String
  createdAt DateTime @default(now())

  @@map("tracks")
}
